#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lenartcernelic/Butelka/butelka-types/node_modules/.pnpm/payload@3.42.0_graphql@16.11.0_typescript@5.8.3/node_modules/payload/node_modules:/Users/lenartcernelic/Butelka/butelka-types/node_modules/.pnpm/payload@3.42.0_graphql@16.11.0_typescript@5.8.3/node_modules:/Users/lenartcernelic/Butelka/butelka-types/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/lenartcernelic/Butelka/butelka-types/node_modules/.pnpm/payload@3.42.0_graphql@16.11.0_typescript@5.8.3/node_modules/payload/node_modules:/Users/lenartcernelic/Butelka/butelka-types/node_modules/.pnpm/payload@3.42.0_graphql@16.11.0_typescript@5.8.3/node_modules:/Users/lenartcernelic/Butelka/butelka-types/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin.js" "$@"
else
  exec node  "$basedir/../../bin.js" "$@"
fi
