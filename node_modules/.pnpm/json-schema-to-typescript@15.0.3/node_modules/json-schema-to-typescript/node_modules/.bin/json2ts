#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lenartcernelic/Butelka/butelka-types/node_modules/.pnpm/json-schema-to-typescript@15.0.3/node_modules/json-schema-to-typescript/dist/src/node_modules:/Users/lenartcernelic/Butelka/butelka-types/node_modules/.pnpm/json-schema-to-typescript@15.0.3/node_modules/json-schema-to-typescript/dist/node_modules:/Users/lenartcernelic/Butelka/butelka-types/node_modules/.pnpm/json-schema-to-typescript@15.0.3/node_modules/json-schema-to-typescript/node_modules:/Users/lenartcernelic/Butelka/butelka-types/node_modules/.pnpm/json-schema-to-typescript@15.0.3/node_modules:/Users/lenartcernelic/Butelka/butelka-types/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/lenartcernelic/Butelka/butelka-types/node_modules/.pnpm/json-schema-to-typescript@15.0.3/node_modules/json-schema-to-typescript/dist/src/node_modules:/Users/lenartcernelic/Butelka/butelka-types/node_modules/.pnpm/json-schema-to-typescript@15.0.3/node_modules/json-schema-to-typescript/dist/node_modules:/Users/lenartcernelic/Butelka/butelka-types/node_modules/.pnpm/json-schema-to-typescript@15.0.3/node_modules/json-schema-to-typescript/node_modules:/Users/lenartcernelic/Butelka/butelka-types/node_modules/.pnpm/json-schema-to-typescript@15.0.3/node_modules:/Users/lenartcernelic/Butelka/butelka-types/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/src/cli.js" "$@"
else
  exec node  "$basedir/../../dist/src/cli.js" "$@"
fi
